{
    "name": "nepada/file-upload-control",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "File upload control for Nette forms: multi-file AJAX upload via blueimp-file-upload, Bootstrap 4 and 5 template.",
    "keywords": ["file", "upload", "form", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/file-upload-control/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.5",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "latte/latte": "^3.0.12@dev",
        "nette/application": "^3.1.9@dev",
        "nette/forms": "^3.2@dev",
        "nette/http": "^3.3@dev",
        "nette/utils": "^4.0.4@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.4",
        "mockery/mockery": "1.6.12",
        "phpstan/phpstan": "2.1.17",
        "phpstan/phpstan-strict-rules": "2.0.4",
        "phpstan/phpstan-nette": "2.0.3",
        "nepada/phpstan-nette-tester": "2.0.0",
        "spaze/phpstan-disallowed-calls": "4.5.0",
        "shipmonk/phpstan-rules": "4.1.4",
        "phpstan/phpstan-mockery": "2.0.0",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "7.15",
        "nette/di": "^3.0.6@dev",
        "nette/bootstrap": ">=3.1@dev",
        "composer/semver": "3.4.3",
        "nette/neon": ">=3.3.4@dev",
        "nette/robot-loader": "*@dev"
    },
    "suggest": {
        "ext-gd": "to use image thumbnails",
        "ext-exif": "to enable auto-rotation of image thumbnails"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\FileUploadControl\\": "src/FileUploadControl/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.9-dev"
        }
    }
}
